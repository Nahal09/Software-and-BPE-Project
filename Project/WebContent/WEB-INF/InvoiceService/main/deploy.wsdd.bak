<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from InvoiceService WSDL service -->

  <service name="Invoice" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://main"/>
      <parameter name="wsdlServiceElement" value="InvoiceService"/>
      <parameter name="schemaQualified" value="http://main"/>
      <parameter name="wsdlServicePort" value="Invoice"/>
      <parameter name="className" value="main.InvoiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Invoice"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="createInvoice" qname="operNS:createInvoice" xmlns:operNS="http://main" soapAction="" >
        <parameter qname="pns:personnelContact" xmlns:pns="http://main" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:foodName" xmlns:pns="http://main" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:foodPrice" xmlns:pns="http://main" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:orderID" xmlns:pns="http://main" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:deliveryID" xmlns:pns="http://main" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="createInvoice"/>

  </service>
</deployment>
